  prettyprint-metaborgc-Decl :
    Decl(t1__, t2__) -> [ H(
                            [SOpt(HS(), "0")]
                          , [ t1__'
                            , S(" ")
                            , t2__'
                            , S(";")
                            ]
                          )
                        ]
    with t1__' := <pp-one-Z(prettyprint-metaborgc-DeclSpecs) <+ pp-one-Z(prettyprint-completion-aux)> t1__
    with t2__' := <pp-H-list(prettyprint-metaborgc-InitDeclarator|", ")
                   <+ pp-one-Z(prettyprint-completion-aux)> t2__

  is-metaborgc-Decl =
    ?Decl(_, _)

  metaborgc-get-indent :
    (t@Decl(t1__, t2__), index) -> indent
    where sort := <ia-get-sort> t
          ; <?"Decl"> sort
    with switch !index
           case ?1 :
             indent := ""
           case ?2 :
             indent := ""
           otherwise :
             indent := ""
         end

  metaborgc-get-separator :
    (t@Decl(t1__, t2__), index) -> separator
    where sort := <ia-get-sort> t
          ; <?"Decl"> sort
    with switch !index
           case ?1 :
             separator := ""
           case ?2 :
             separator := ", "
           otherwise :
             separator := ""
         end

  metaborgc-get-leading-ws :
    (t@Decl(t1__, t2__), index) -> ws
    where sort := <ia-get-sort> t
          ; <?"Decl"> sort
    with switch !index
           case ?1 :
             ws := ""
           case ?2 :
             ws := " "
           otherwise :
             ws := ""
         end

